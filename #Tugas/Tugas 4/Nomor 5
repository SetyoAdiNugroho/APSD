#include <iostream>

using namespace std;

int main() {
    int start = 10;
    int sum = 0;

    for (int a = 5; a >= 1; --a) {
        for (int b = 1; b <= a; ++b) {
            cout << start;
            sum += start;
            if (b < a) {
                cout << " + ";
            }
            start -= 2;
        }
        cout << " = " << sum << endl;
        sum = 0;
        start = 10;
        }
        cout << "-----------" << endl;
        cout << "110" << endl;
        
    return 0;
}
